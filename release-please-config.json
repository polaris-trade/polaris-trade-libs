      "release-type": "rust",
      "package-name": "logger",
      "changelog-path": "CHANGELOG.md"
    },
    "crates/utils/task-manager": {
      "release-type": "rust",
      "package-name": "task-manager",
      "changelog-path": "CHANGELOG.md"
